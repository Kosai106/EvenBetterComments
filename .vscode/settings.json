// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**.csproj": true,
		"**.meta": true,
		"**.cs.meta": true,
		"out": true,
		"npm-debug.log": true,
		"**/node_modules": true
	},
    "search.exclude": {
        "out": true // set this to false to include "out" folder in search results
	},
	"vsicons.presets.angular": false,
	"explorerExclude.backup": {}
}